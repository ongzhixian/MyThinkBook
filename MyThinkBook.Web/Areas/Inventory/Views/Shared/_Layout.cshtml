<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MyThinkBook</title>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="/css/materialize/materialize.min.css" media="screen,projection" type="text/css" />
    <link rel="stylesheet" href="~/css/wms-site.css" asp-append-version="true" type="text/css" />
    <link rel="shortcut icon" type="image/x-icon" href="/favicon.ico" />
</head>

<body>
    @RenderBody()
    <script type="text/javascript" src="/js/materialize/materialize.min.js"></script>
    <script src="~/js/signalr/dist/browser/signalr.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>