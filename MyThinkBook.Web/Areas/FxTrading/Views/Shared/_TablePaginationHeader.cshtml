@using MyThinkBook.Web.Domain;
@model IPageMetaData
<div class="row pagination">
    <div class="one-half column">
        Record @Model.RecordStart - @Model.RecordEnd of @Model.TotalRecordCount
    </div>
    <div class="one-half column" style="text-align:right;">
        Page @Model.CurrentPage of @Model.TotalPageCount
    </div>
</div>